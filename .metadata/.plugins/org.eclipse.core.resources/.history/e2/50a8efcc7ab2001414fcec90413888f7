package tictactoe;

public class BoardPosition {

	private Type type = null;
	private int position;

	public BoardPosition(int position) {
		this.position = position;		
	}
	
	public void setType(Type type){
		this.type = type;
	}
	
	@Override
	public String toString() {
		String s = "";
		if (type.equals(Type.X)){
			s+="X";
		}else s += "O";
		return s;
	}
}
